{% extends "layout.twig" %}

{% block templatehead %}
{% endblock %}

{% block content %}

<!-- CONTAINING PAGE WRAP-->
<div class="page-wrap">

    <div class="bk-pt-default">

    {% include '/includes/rd-header--alt.twig' %}

    <!-- SUB NAV BAR !-->
    <section class="partners-subnav">
        <div class="wrapper">
        	{{widget('content', 'subavheader', {"content":'<h2 class="hwhite">Site Maker XL contact center</h2>'})|raw}}
        </div>
    </section>

    <!-- THANK YOU - DESKTOP !-->
    <section class=" thankyou LightGrey">
        <div class="wrapper">
            <div class="form-success">
                <span>
                    <svg version="1.1" id="Layer_1" xmlns="https://www.w3.org/2000/svg" xmlns:xlink="https://www.w3.org/1999/xlink" x="0px" y="0px"
                     width="200px" height="150px" viewBox="-48.641 -51.976 200 150" enable-background="new -48.641 -51.976 200 150"
                     xml:space="preserve">
                    <polyline class="check" fill="none" stroke="#FFFFFF" stroke-width="30" stroke-linecap="round" stroke-linejoin="round" stroke-miterlimit="5" points="
                    -25.023,22.098 27.315,74.436 130.14,-28.389 "/>
                </svg>
                </span>
                {{widget('content', 'thankyoutext', {"content":'<h2 class="h2-36pad text-center">Thank you for passing on your interest!</h2>'})|raw}}
            </div>
        </div>
    </section>

    <!-- PARTNER BAR !-->
    <section class="partner-bar bottom white">
        <div class="wrapper">
            <!-- BAR - DESKTOP !-->
            <div class="desktop">
                <div class="res-col columns span-12-21x3-27">
                    <div class="column span-five-first">
                        {{widget('content', 'partnerbar1content1', {"content":'<h4 class="h4light">Proud of our<br> customers like</h4>'})|raw}}
                    </div>
                    <div class="column span-five">
                        <img src="{{asset('/images/partner-logos/host-europe.png')}}">
                    </div>
                    <div class="column span-five">
                        <img src="{{asset('/images/partner-logos/123-reg.png')}}" class="reg-logo">
                    </div>
                    <div class="column span-five">
                        <img src="{{asset('/images/partner-logos/fasthosts.png')}}" class="enom-logo">
                    </div>
                    <div class="column span-five-last">
                        <a href="/partners" class="button main-btn green">Start with your own website</a>
                    </div>
                </div>
            </div>
            <!-- BAR - TABLET !-->
            <div class="partner-res">
                {{widget('content', 'partnerbar2content1', {"content":'<h3><span class="mobHide">Start your Site Maker XL Demo now for free, <span class="darkspan">without risk</span>!</span><a href="/partners" class="button main-btn green">Start with your website</a></h3>'})|raw}}
            </div>
        </div>
    </section>

    </div>

<script type="text/javascript">
    $(document).ready(function () {
        $('.check').css('stroke-dashoffset', 0);
    });
</script>

{% include '/includes/rd-footer.twig' %}

<!-- END CONTAINING PAGE WRAP-->
</div>

{% endblock %}
